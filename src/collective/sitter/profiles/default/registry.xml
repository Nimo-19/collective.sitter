<?xml version="1.0"?>
<registry>

  <records interface="collective.sitter.browser.controlpanel.ISitterSettings"
           prefix="sitter" />

  <record name="plone.displayed_types">
    <value purge="False">
      <element>sitterfolder</element>
    </value>
  </record>

  <records
      interface="eea.facetednavigation.interfaces.IEEASettings"
      prefix="eea.facetednavigation.interfaces.IEEASettings">
    <value key="disable_diazo_rules_ajax">True</value>
  </records>

  <records prefix="plone.bundles/collective.sitter"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
    </value>
    <value key="depends">faceted-view</value>
    <value key="enabled">True</value>
    <value key="compile">False</value>
    <value key="jscompilation"></value>
    <value key="csscompilation">++plone++collective.sitter/collective.sitter.css</value>
  </records>

</registry>
